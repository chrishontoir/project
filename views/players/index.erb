<div class="view_players_title"><p>Players</p></div>

<div class="view_players_content">
  <% for player in @players %>
  <div class="player_template">
    <div class="player_template_avatar"><img src="<%=player.avatar%>" alt=""></div>
    <div class=player_template_name><%=player.name%></div>
    <br>
    <div class="player_template_options"><a href="/players/<%=player.id%>">View Profile</a> | <a href="/players/<%=player.id%>/delete"><span>Delete Profile</span></a></div>
  </div>
  <% end %>

  </div>

<div class="select"><a href="/players/new">New Player</a><a href="/">Back</a></div>
